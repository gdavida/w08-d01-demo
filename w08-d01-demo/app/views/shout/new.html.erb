<h1>Shout#new</h1>
<ul>
	<li><%= link_to "Home", root_path  %></li>
	<li><%= link_to "Shouty Greeter", new_shout_path %></li>
	<li><%= link_to "Fizz Buzz", new_fizzbuzz_path %></li>
	<li><%= link_to "MySpace Name Generator", new_myspace_path %>
</ul>



<%= form_tag(shout_path, method: :get) do %>
  <div>
    <%= label_tag :name, "Name" %>
    <%= text_field_tag :name  %>
  </div>
  <div>
    <%= submit_tag "Submit" %>
  </div>
<% end %>